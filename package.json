{
  "name": "drino-rx",
  "version": "0.0.0",
  "description": "Plugin to use Drino with RxJS",
  "license": "MIT",
  "author": {
    "name": "RÃ©my Abitbol",
    "url": "https://github.com/remscodes"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run start-web-runner",
    "types": "tsc --noEmit",
    "types:spec": "npm run types -- -p ./tsconfig.spec.json",
    "types:all": "npm run types & npm run types:spec",
    "start-web-runner": "web-test-runner",
    "rm-dist": "node ./scripts/rm-dist.cjs",
    "playwright": "node ./node_modules/.pnpm/playwright@1.38.0/node_modules/playwright/cli.js",
    "install-playwright": "npm run playwright -- install --with-deps",
    "prebuild": "npm run rm-dist",
    "pretest": "npm run types:all"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remscodes/drino-rx"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "peerDependencies": {
    "drino": "^1.4.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.17.15",
    "@types/sinon": "^10.0.16",
    "@web/dev-server-esbuild": "^0.4.1",
    "@web/test-runner": "^0.17.1",
    "@web/test-runner-playwright": "^0.10.1",
    "helmet": "^7.0.0",
    "sinon": "^15.2.0",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "drino",
    "rxjs",
    "plugin"
  ],
  "packageManager": "pnpm@8.7.5",
  "sideEffects": false
}
